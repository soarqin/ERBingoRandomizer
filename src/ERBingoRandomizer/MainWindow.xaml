                    <CheckBox Margin="0,5,0,0" Width="280" Content="随机初始职业" VerticalAlignment="Center"
                              IsChecked="{Binding RandomStartupClasses, Mode=TwoWay}"
                              FontSize="13" FontWeight="Medium" />
                    <CheckBox Width="280" Content="随机武器" VerticalAlignment="Center"
                              IsChecked="{Binding RandomWeapons, Mode=TwoWay}"
                              FontSize="13" FontWeight="Medium" />
                    <CheckBox Width="280" Content="获取灵马后开放赐福" VerticalAlignment="Center"
                              IsChecked="{Binding OpenGraces, Mode=TwoWay}"
                              FontSize="13" FontWeight="Medium" />
                    <CheckBox Width="280" Content="降低非失色升级材料需求" VerticalAlignment="Center"
                              IsChecked="{Binding ReduceUpgradeMat, Mode=TwoWay}"
                              FontSize="13" FontWeight="Medium" />
                    <RadioButton Margin="50,0,0,0" Width="280" Content="1/1/1" VerticalAlignment="Center" GroupName="ReduceUpgradeMatGroup"
                                 IsChecked="{Binding IsReduceUpgradeMatType0, Mode=TwoWay}"
                                 IsEnabled="{Binding ReduceUpgradeMat}"
                                 FontSize="13" FontWeight="Medium" />
                    <RadioButton Margin="50,0,0,0" Width="280" Content="1/2/3" VerticalAlignment="Center" GroupName="ReduceUpgradeMatGroup"
                                 IsChecked="{Binding IsReduceUpgradeMatType1, Mode=TwoWay}"
                                 IsEnabled="{Binding ReduceUpgradeMat}"
                                 FontSize="13" FontWeight="Medium" />
                    <CheckBox Width="280" Content="随机掉落装备的概率提升到50%" VerticalAlignment="Center"
                              IsChecked="{Binding GreaterItemLootChance, Mode=TwoWay}"
                              FontSize="13" FontWeight="Medium" />
