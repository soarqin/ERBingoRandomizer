        <Grid.RowDefinitions>
            <RowDefinition Height="Auto" />
            <RowDefinition Height="Auto" />
            <RowDefinition Height="Auto" />
            <RowDefinition />
        </Grid.RowDefinitions>
        <StackPanel>
            <Separator Height="0" Margin="0,20,0,0" />
            <StackPanel Orientation="Horizontal" HorizontalAlignment="Center">
                <Label Width="120" HorizontalContentAlignment="Right" HorizontalAlignment="Center"
                       Content="艾尔登法环exe路径：" FontWeight="UltraBlack">
                </Label>
                <TextBox Width="420" Text="{Binding Path, UpdateSourceTrigger=PropertyChanged}"
                         VerticalContentAlignment="Center" FontSize="13" FontWeight="Bold" />
            </StackPanel>
            <Separator Height="0" Margin="0,20,0,0" />
            <StackPanel Orientation="Horizontal" HorizontalAlignment="Center">
                <Label Width="120" HorizontalContentAlignment="Right" Content="输入种子：" HorizontalAlignment="Center"
                       FontWeight="UltraBlack" />
                <TextBox Margin="0, 0, 220, 0" Width="200" Text="{Binding Seed, UpdateSourceTrigger=PropertyChanged}"
                         VerticalContentAlignment="Center" FontSize="13" FontWeight="Bold" />
            </StackPanel>
        </StackPanel>
